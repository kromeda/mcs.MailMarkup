@page "/fastconverter/recognized"
@model AllRecognizedModel
@{
    ViewData["Title"] = "Уведомление";
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,500&display=swap&subset=cyrillic" rel="stylesheet" />

    <title>Уведомление</title>
</head>
<body style="margin: 0; padding: 0; background-color:#f0f3f4; padding:70px 0">
    <!--[if (gte mso 9)|(IE)]>
    <table width="600" align="center"><tr><td style="padding-top:0; padding-bottom:0; padding-right:0; padding-left:0; margin:0px;">
    <![endif]-->
    <table align="center" border="0" cellpadding="0" cellspacing="0" width="600" style="border-collapse:collapse; margin: 0 auto; font: 18px/28px 'Roboto'; background-color: #fff;">
        <tr>
            <td>
                <img alt="ТНС энерго" src="https://cdn.tns-e.ru/medialibrary/bc5/bc58718df00461e6dbc3b374057df5bd/logo.jpg" title="ТНС энерго" border="0" />
            </td>
        </tr>
        <tr>
            <td>
                <table border="0" cellpadding="0" cellspacing="0" width="100%">
                    <tr>
                        <td>
                            <p style="font-size: 24px; text-align: center;">
                                <i><b>Уважаемый потребитель!</b></i>
                            </p>
                            <p style="text-indent: 20px; padding-left: 15px;">
                                <i>
                                    Предоставленные Вами данные о показаниях прибора учета получены. Отвечать на данное письмо не нужно. Спасибо!
                                </i>
                            </p>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <i style="text-indent: 20px;padding: 0 0 15px 15px;">С Уважением, @Model.OrganizationName.</i>
                        </td>
                    </tr>
                    <tr style="background-color:#fff; border:1px solid #e0e0e0; border-top:1px solid #f0f3f4; box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.1);  color:#6e6e6e; font-size:13px; line-height:22px;">
                        <td style="padding: 10px; background: #13a438;">
                            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                <tr>

                                    <td width="400" valign="top" style="color: #000;">
                                        Это письмо сформировано автоматически, отвечать на него не нужно.
                                        Если это сообщение пришло к Вам по ошибке, пожалуйста, проигнорируйте его.
                                        С уважением, @Model.OrganizationName.
                                    </td>
                                    <td align="right" width="40px" valign="middle">
                                        <img src="data:image/png;base64," />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>

    <!--[if (gte mso 9)|(IE)]>
       </td></tr></table><![endif]-->
</body>
</html>